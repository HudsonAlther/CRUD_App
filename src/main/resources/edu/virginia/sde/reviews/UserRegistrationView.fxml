<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.controller.UserRegistrationController">
    <Label text="User Registration" style="-fx-font-size: 20px;" />
    <TextField fx:id="usernameField" promptText="Enter Username" />
    <PasswordField fx:id="passwordField" promptText="Enter Password" />
    <Button text="Register" onAction="#handleCreateUser" />
    <Button text="Back to Login" onAction="#handleBackToLogin" />
</VBox>
